{
  "extends": "airbnb",
  "rules": {
    "import/extensions": [1, { "js": "never", "json": "always" }],
    "semi": [
      1,
      "always"
    ],
    "indent": [
      1,
      4
    ],
    "space-before-function-paren": [
      1,
      {
        "anonymous": "always",
        "named": "always"
      }
    ],
    "no-console": 0,
    "padded-blocks": 0,
    "no-empty": 0
  }
}
