{
  "extends": "airbnb",
  "rules": {
    "import/extensions": [1, { "js": "never", "json": "always" }],
    "semi": [
      1,
      "always"
    ],
    "indent": [
      1,
      4
    ],
    "space-before-function-paren": [
      1,
      {
        "anonymous": "always",
        "named": "always"
      }
    ],
    "no-new": 0,
    "no-console": 0,
    "padded-blocks": 0,
    "no-empty": 0,
    "no-param-reassign": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^h$" }],
    "react/react-in-jsx-scope": 0,
    "react/jsx-indent": 0,
    "react/jsx-filename-extension": 0,
    "react/no-unknown-property": 0
  }
}
