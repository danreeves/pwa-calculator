{
  "extends": "airbnb",
  "rules": {
    "import/extensions": [
      1,
      {
        "js": "never",
        "json": "always"
      }
    ],
    "semi": [
      1,
      "always"
    ],
    "indent": [
      1,
      4
    ],
    "space-before-function-paren": [
      1,
      {
        "anonymous": "always",
        "named": "always"
      }
    ],
    "react/jsx-filename-extension": 0,
    "react/jsx-indent": 0,
    "import/no-extraneous-dependencies": 0,
    "no-empty": ["error", { "allowEmptyCatch": true }]
  }
}
